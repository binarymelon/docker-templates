<?xml version="1.0" encoding="UTF-8"?>
<Containers>
  <TemplateURL>https://gitlab.com/binarymelon/docker-templates/-/raw/master/binarymelon/qcma.xml</TemplateURL>
  <Beta>True</Beta>
  <Category>HomeAutomation:</Category>
  <Name>Nanit Stream Proxy</Name>
  <Description>This is sleepless night induced pet project to restream Nanit Baby Monitor live stream for local viewing.</Description>
  <Registry>https://registry.gitlab.com/adam.stanek/nanit</Registry>
	<Repository>adam.stanek/nanit</Repository>
  <Project>https://github.com/codestation/qcma</Project>
  <BindTime>true</BindTime>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>1935</HostPort>
        <ContainerPort>1935</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>NANIT_EMAIL</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>NANIT_PASSWORD</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>NANIT_RTMP_ENABLED</Name>
      <Value>true</Value>
    </Variable>
    <Variable>
      <Name>NANIT_RTMP_ADDR</Name>
      <Value>xxx.xxx.xxx.xxx:1935</Value>
    </Variable>
  </Environment>
  <Data>
  </Data>
</Containers>
